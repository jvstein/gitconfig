*.gif   diff=image
*.GIF   diff=image
*.ico   diff=image
*.jpg   diff=image
*.JPG   diff=image
*.jpeg  diff=image
*.JPEG  diff=image
*.pdf   diff=image
*.png   diff=image
*.PNG   diff=image
*.tiff  diff=image
*.TIFF  diff=image
